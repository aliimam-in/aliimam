<% /* D. No variants */ %>
<% } else { %>
export const <%- iconToComponentName(icon) %> = React.forwardRef<SVGSVGElement, IconProps>(
  function <%- iconToComponentName(icon) %>({
    color = 'currentColor',
    fill,
    strokeWidth,
    strokeLinecap,
