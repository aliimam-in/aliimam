  "files": [
    {
      "path": "registry/default/example/alert-dialog/alert-dialog-01.tsx",
      "content": "\"use client\";\n\nimport * as React from \"react\";\nimport {\n  AlertDialog,\n  AlertDialogTrigger,\n  AlertDialogContent,\n  AlertDialogHeader,\n  AlertDialogFooter,\n  AlertDialogTitle,\n  AlertDialogDescription,\n  AlertDialogCancel,\n  AlertDialogAction,\n} from \"@/registry/default/ui/alert-dialog\";\nimport { Button } from \"@/registry/default/ui/button\";\nimport { Info } from \"@aliimam/icons\";\n\nexport function AlertDialogBasic() {\n  return (\n    <div className=\"flex gap-2\">\n      <AlertDialog>\n        <AlertDialogTrigger asChild>\n          <Button size={\"icon\"} variant=\"outline\"><Info/></Button>\n        </AlertDialogTrigger>\n        <AlertDialogContent>\n          <AlertDialogHeader>\n            <AlertDialogTitle>Are you absolutely sure?</AlertDialogTitle>\n            <AlertDialogDescription>\n              This action cannot be undone. This will permanently delete your\n              account and remove all associated data.\n            </AlertDialogDescription>\n          </AlertDialogHeader>\n          <AlertDialogFooter>\n            <AlertDialogCancel>Cancel</AlertDialogCancel>\n            <AlertDialogAction>Continue</AlertDialogAction>\n          </AlertDialogFooter>\n        </AlertDialogContent>\n        <AlertDialogTrigger asChild>\n          <Button variant=\"default\">Show Confirmation</Button>\n        </AlertDialogTrigger>\n        <AlertDialogContent>\n          <AlertDialogHeader>\n            <AlertDialogTitle>Are you absolutely sure?</AlertDialogTitle>\n            <AlertDialogDescription>\n              This action cannot be undone. This will permanently delete your\n              account and remove all associated data.\n            </AlertDialogDescription>\n          </AlertDialogHeader>\n          <AlertDialogFooter>\n            <AlertDialogCancel>Cancel</AlertDialogCancel>\n            <AlertDialogAction>Continue</AlertDialogAction>\n          </AlertDialogFooter>\n        </AlertDialogContent> \n      </AlertDialog>\n    </div>\n  );\n}\n",
      "type": "registry:example"
    }
  ]
}