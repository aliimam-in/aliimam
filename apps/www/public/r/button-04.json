      "content": "import { Button } from \"@/registry/default/ui/button\";\nimport { Mail } from \"@aliimam/icons\";\nimport {\n  Facebook,\n  Bluesky,\n  Github,\n  Google,\n  WhatsApp,\n  Instagram,\n  X,\n  LinkedIn,\n  Threads,\n} from \"@aliimam/logos\";\n\nexport default function ButtonDemo() {\n  return (\n    <div className=\"flex max-w-lg flex-wrap gap-3\">\n      <Button className=\"w-full\">Login</Button>\n      <Button variant={\"outline\"} className=\"w-full\">Create account</Button>\n      <div className=\"grid w-full gap-3 grid-cols-3\">\n        <Button className=\"w-full\" variant=\"outline\" size=\"icon\">\n          <Google />\n        </Button>\n        <Button className=\"w-full\" variant=\"outline\" size=\"icon\">\n          <Github />\n        </Button>\n        <Button className=\"w-full\" variant=\"outline\" size=\"icon\">\n          <Facebook />\n        </Button>\n      </div>\n      <Button size=\"icon-lg\">\n        <Instagram />\n      </Button>\n      <Button size=\"icon-lg\">\n        <X />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <LinkedIn />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <Google />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <Github />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <Facebook />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <Bluesky />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <WhatsApp />\n      </Button>\n      <Button variant=\"outline\" size=\"icon-lg\">\n        <Threads />\n      </Button>\n      <Button variant=\"outline\">\n        <Google />\n        Login with Google\n      </Button>\n      <Button variant=\"outline\">\n        <Github />\n        Signup with Github\n      </Button>\n      <Button>\n        <X />\n        Continue with X\n      </Button>\n      <Button variant={\"secondary\"}>\n        <Mail />\n        Login with email\n      </Button>\n      <Button variant={\"outline\"}>\n        <svg viewBox=\"0 0 814 1000\">\n          <path\n            fill=\"currentColor\"\n            d=\"M788.1 340.9c-5.8 4.5-108.2 62.2-108.2 190.5 0 148.4 130.3 200.9 134.2 202.2-.6 3.2-20.7 71.9-68.7 141.9-42.8 61.6-87.5 123.1-155.5 123.1s-85.5-39.5-164-39.5c-76.5 0-103.7 40.8-165.9 40.8s-105.6-57-155.5-127C46.7 790.7 0 663 0 541.8c0-194.4 126.4-297.5 250.8-297.5 66.1 0 121.2 43.4 162.7 43.4 39.5 0 101.1-46 176.3-46 28.5 0 130.9 2.6 198.3 99.2zm-234-181.5c31.1-36.9 53.1-88.1 53.1-139.3 0-7.1-.6-14.3-1.9-20.1-50.6 1.9-110.8 33.7-147.1 75.8-28.5 32.4-55.1 83.6-55.1 135.5 0 7.8 1.3 15.6 1.9 18.1 3.2.6 8.4 1.3 13.6 1.3 45.4 0 102.5-30.4 135.5-71.3z\"\n          />\n        </svg>\n        Login with Apple\n      </Button>\n      <div className=\"flex gap-2 items-center\">\n        Dont have an account?\n        <Button className=\"px-0\" variant={\"link\"}>\n          Sign up\n        </Button>\n      </div>\n    </div>\n  );\n}\n",
      "type": "registry:example"
    }
