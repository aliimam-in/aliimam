  "files": [
    {
      "path": "blocks/header/header-02/page.tsx",
      "content": "import { Header } from \"@/registry/aliimam/blocks/header/header-02/components/header\"\n\nexport default function Header02() {\n  return (\n    <div className=\"flex min-h-screen w-full flex-col\">\n      <Header />\n      <main className=\"flex-1\">\n        <section className=\"flex h-screen items-center justify-center\">\n          <h1 className=\"text-4xl font-bold\">Hero</h1>\n        </section>\n\n        <section className=\"flex h-screen items-center justify-center\">\n          <h1 className=\"text-4xl font-bold\">Section</h1>\n        </section>\n      </main>\n    </div>\n  )\n}\n",
      "type": "registry:component",
      "target": "components/header-02.tsx"
    },
    {
      "path": "blocks/header/header-02/components/header.tsx",
      "content": "\"use client\"\n\nimport { useState } from \"react\"\nimport { ArrowRight, Equal } from \"@aliimam/icons\"\n\nimport { Search } from \"@/registry/aliimam/blocks/header/header-02/components/search\"\nimport { ModeToggle } from \"@/registry/aliimam/blocks/header/header-02/components/theme-switch\"\nimport {\n  Accordion,\n  AccordionContent,\n  AccordionItem,\n  AccordionTrigger,\n} from \"@/components/ui/accordion\"\nimport { Button } from \"@/components/ui/button\"\nimport { Sheet, SheetContent, SheetTrigger } from \"@/components/ui/sheet\"\n\nexport function Header() {\n  const [mobileOpen, setMobileOpen] = useState(false)\n\n  const navItems = [\n    { label: \"Overview\", href: \"#\", active: true },\n    { label: \"Design Systems\", href: \"#\" },\n    { label: \"UI Kits\", href: \"#\" },\n    { label: \"Brand Assets\", href: \"#\" },\n    { label: \"Templates\", href: \"#\" },\n    { label: \"Workshops\", href: \"#\" },\n    { label: \"Design Labs\", href: \"#\" },\n    { label: \"Case Studies\", href: \"#\" },\n    { label: \"Resources\", href: \"#\" },\n  ]\n\n  const properties = [\n    {\n      location: \"Design Hub – India\",\n      venues: [{ name: \"Creative Studio\", href: \"#\" }],\n    },\n    {\n      location: \"Design Hub – Global\",\n      venues: [\n        { name: \"UI/UX Studio\", href: \"#\" },\n        { name: \"Branding Lab\", href: \"#\" },\n      ],\n    },\n  ]\n\n  return (\n    <>\n      <header className=\"bg-background sticky top-0 z-50 border-b\">\n        <div className=\"mx-auto px-4 sm:px-6 lg:px-8\">\n          <div className=\"flex h-16 items-center justify-between lg:h-20\">\n            <a\n              href=\"#\"\n              aria-label=\"home\"\n              className=\"flex items-center gap-2 whitespace-nowrap\"\n            >\n              <img\n                src=\"/ai-logo-white.png\"\n                alt=\"Design Logo\"\n                height={50}\n                width={50}\n                className=\"z-10 hidden h-10 w-full object-contain dark:block\"\n              />\n              <img\n                src=\"/ai-logo-black.png\"\n                alt=\"Design Logo\"\n                height={50}\n                width={50}\n                className=\"z-10 block h-10 w-full object-contain dark:hidden\"\n              />\n              <span className=\"text-xl font-semibold\">Ali Imam</span>\n            </a>\n\n            <Search />\n\n            <div className=\"flex items-center gap-2\">\n              <ModeToggle />\n              <Button className=\"hidden items-center gap-2 md:flex\">\n                <span>Start Designing</span>\n                <ArrowRight className=\"h-4 w-4\" />\n              </Button>\n\n              <Sheet open={mobileOpen} onOpenChange={setMobileOpen}>\n                <SheetTrigger asChild>\n                  <Button variant=\"ghost\" size=\"icon\" className=\"lg:hidden\">\n                    <Equal className=\"h-6 w-6\" />\n                  </Button>\n                </SheetTrigger>\n                <SheetContent\n                  side=\"right\"\n                  className=\"w-full overflow-y-auto sm:w-[400px]\"\n                >\n                  <div className=\"m-6 mt-14\">\n                    <Accordion type=\"single\" collapsible className=\"mb-8\">\n                      <AccordionItem value=\"navigation\" className=\"border-none\">\n                        <AccordionTrigger className=\"bg-accent hover:bg-accent/80 rounded-lg px-4 hover:no-underline\">\n                          <div className=\"text-left\">\n                            <div className=\"font-medium\">Design Hub</div>\n                            <div className=\"text-muted-foreground text-sm\">\n                              / Overview\n                            </div>\n                          </div>\n                        </AccordionTrigger>\n                        <AccordionContent className=\"pt-2\">\n                          <div className=\"overflow-hidden rounded-lg border\">\n                            {navItems.map((item, idx) => (\n                              <a\n                                key={idx}\n                                href={item.href}\n                                className={`hover:bg-accent block border-b px-4 py-3 text-sm transition-colors last:border-b-0 ${\n                                  item.active ? \"bg-accent font-medium\" : \"\"\n                                }`}\n                                onClick={() => setMobileOpen(false)}\n                              >\n                                {item.label}\n                              </a>\n                            ))}\n                          </div>\n                        </AccordionContent>\n                      </AccordionItem>\n                    </Accordion>\n\n                    <div className=\"space-y-6\">\n                      {properties.map((property, idx) => (\n                        <div key={idx}>\n                          <p className=\"text-muted-foreground mb-2 text-xs tracking-wider uppercase\">\n                            {property.location}\n                          </p>\n                          <ul className=\"space-y-2\">\n                            {property.venues.map((venue, vIdx) => (\n                              <li key={vIdx} className=\"border-t border-black\">\n                                <a\n                                  href={venue.href}\n                                  className=\"hover:text-muted-foreground block py-3 text-lg font-medium transition-colors\"\n                                  onClick={() => setMobileOpen(false)}\n                                >\n                                  {venue.name}\n                                </a>\n                              </li>\n                            ))}\n                          </ul>\n                        </div>\n                      ))}\n                    </div>\n                  </div>\n                </SheetContent>\n              </Sheet>\n            </div>\n          </div>\n\n          {/* Desktop Secondary Navigation */}\n          <nav className=\"hidden border-t lg:block\">\n            <ul className=\"-mx-2 flex items-center justify-center\">\n              {navItems.map((item, idx) => (\n                <li key={idx}>\n                  <a\n                    href={item.href}\n                    className={`block px-4 py-3 text-sm transition-colors ${\n                      item.active\n                        ? \"border-primary border-b-2 font-semibold\"\n                        : \"hover:bg-primary/20\"\n                    }`}\n                  >\n                    {item.label}\n                  </a>\n                </li>\n              ))}\n            </ul>\n          </nav>\n        </div>\n      </header>\n    </>\n  )\n}\n",
      "type": "registry:component"
    },
    {
      "path": "blocks/header/header-02/components/theme-switch.tsx",
      "content": "\"use client\"\n\nimport * as React from \"react\"\nimport { useEffect, useState } from \"react\"\nimport { Moon, Sun } from \"@aliimam/icons\"\nimport { useTheme } from \"next-themes\"\n\nimport { Toggle } from \"@/components/ui/toggle\"\n\nexport function ModeToggle() {\n  const { theme, setTheme } = useTheme()\n  const [mounted, setMounted] = useState(false)\n\n  useEffect(() => {\n    setMounted(true)\n  }, [])\n\n  if (!mounted) return null\n\n  return (\n    <div className=\"flex flex-col justify-center\">\n      <div>\n        <Toggle\n          className=\"group bg-secondary dark:bg-secondary data-[state=on]:hover:bg-muted size-9 cursor-pointer data-[state=on]:bg-transparent\"\n          pressed={theme === \"dark\"}\n          onPressedChange={() => setTheme(theme === \"dark\" ? \"light\" : \"dark\")}\n          aria-label={`Switch to ${theme === \"dark\" ? \"light\" : \"dark\"} mode`}\n        >\n          <Moon\n            size={16}\n            className=\"shrink-0 scale-0 opacity-0 transition-all group-data-[state=on]:scale-100 group-data-[state=on]:opacity-100\"\n            aria-hidden=\"true\"\n          />\n          <Sun\n            size={16}\n            className=\"absolute shrink-0 scale-100 opacity-100 transition-all group-data-[state=on]:scale-0 group-data-[state=on]:opacity-0\"\n            aria-hidden=\"true\"\n          />\n        </Toggle>\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:component"
    },
    {
      "path": "blocks/header/header-02/components/search.tsx",
      "content": "\"use client\"\n\nimport * as React from \"react\"\nimport {\n  ArrowUpRightIcon,\n  FilePlus2Icon,\n  LayoutTemplateIcon,\n  PaletteIcon,\n  PenToolIcon,\n  SearchIcon,\n} from \"lucide-react\"\n\nimport {\n  CommandDialog,\n  CommandEmpty,\n  CommandGroup,\n  CommandInput,\n  CommandItem,\n  CommandList,\n  CommandSeparator,\n  CommandShortcut,\n} from \"@/components/ui/command\"\n\nexport function Search() {\n  const [open, setOpen] = React.useState(false)\n\n  React.useEffect(() => {\n    const down = (e: KeyboardEvent) => {\n      if (e.key === \"k\" && (e.metaKey || e.ctrlKey)) {\n        e.preventDefault()\n        setOpen((open) => !open)\n      }\n    }\n\n    document.addEventListener(\"keydown\", down)\n    return () => document.removeEventListener(\"keydown\", down)\n  }, [])\n\n  return (\n    <>\n      <button\n        className=\"border-input bg-background text-foreground placeholder:text-muted-foreground/70 focus-visible:border-ring focus-visible:ring-ring/50 hidden h-9 w-full max-w-lg rounded-md border px-3 py-2 text-sm shadow-xs transition-[color,box-shadow] outline-none focus-visible:ring-[3px] lg:inline-flex\"\n        onClick={() => setOpen(true)}\n      >\n        <span className=\"flex grow items-center\">\n          <SearchIcon\n            className=\"text-muted-foreground/80 -ms-1 me-3\"\n            size={16}\n            aria-hidden=\"true\"\n          />\n          <span className=\"text-muted-foreground/70 font-normal\">\n            Search design tools...\n          </span>\n        </span>\n        <kbd className=\"bg-background text-muted-foreground/70 ms-12 -me-1 inline-flex h-5 max-h-full items-center rounded border px-1 font-[inherit] text-[0.625rem] font-medium\">\n          ⌘K\n        </kbd>\n      </button>\n      <CommandDialog open={open} onOpenChange={setOpen}>\n        <CommandInput placeholder=\"Type to search components, assets, or docs...\" />\n        <CommandList>\n          <CommandEmpty>No matches found.</CommandEmpty>\n\n          <CommandGroup heading=\"Create\">\n            <CommandItem>\n              <FilePlus2Icon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>New project</span>\n              <CommandShortcut className=\"justify-center\">⌘N</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <LayoutTemplateIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>New template</span>\n              <CommandShortcut className=\"justify-center\">⌘T</CommandShortcut>\n            </CommandItem>\n            <CommandItem>\n              <PenToolIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>Start design</span>\n              <CommandShortcut className=\"justify-center\">⌘S</CommandShortcut>\n            </CommandItem>\n          </CommandGroup>\n\n          <CommandSeparator />\n\n          <CommandGroup heading=\"Navigate\">\n            <CommandItem>\n              <ArrowUpRightIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>Go to workspace</span>\n            </CommandItem>\n            <CommandItem>\n              <ArrowUpRightIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>Go to assets</span>\n            </CommandItem>\n            <CommandItem>\n              <ArrowUpRightIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>Go to documentation</span>\n            </CommandItem>\n          </CommandGroup>\n\n          <CommandSeparator />\n\n          <CommandGroup heading=\"Themes\">\n            <CommandItem>\n              <PaletteIcon\n                size={16}\n                className=\"opacity-60\"\n                aria-hidden=\"true\"\n              />\n              <span>Switch theme</span>\n              <CommandShortcut>⌘⇧T</CommandShortcut>\n            </CommandItem>\n          </CommandGroup>\n        </CommandList>\n      </CommandDialog>\n    </>\n  )\n}\n",
      "type": "registry:component"
    }
  ],
  "meta": {
    "iframeHeight": "500px"
  },
  "categories": [
    "header"
  ],
  "type": "registry:block"
}