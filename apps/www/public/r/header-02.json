  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "header-02",
  "description": "A simple header section.",
  "dependencies": [
    "next-themes",
    "@aliimam/icons"
  ],
  "registryDependencies": [
    "button",
